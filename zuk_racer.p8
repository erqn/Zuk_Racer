pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
t=0
no_chickens=10

function _update()
  t+=1
end

function _draw()
  cls()
  spr(1,64,64+sin(t/20)*5)
  for i=1,10 do
    spr(
      2,
      cos(t/100+i/no_chickens)*50+64,
      sin(t/100+i/no_chickens)*50+64
    )
  end
end
